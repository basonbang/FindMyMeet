{
  "name": "findmymeet",
  "type": "module",
  "scripts": {
    "reset": "node --require dotenv/config server/scrapers/meetsScraper.js &&  node --require dotenv server/config/reset.js",
    "serve": "cd server && nodemon --require dotenv/config server.js",
    "dev": "concurrently \"cd client && vite\" \"npm run reset && npm run serve\"",
    "start": "cd server && node server",
    "build": "cd client && vite build"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.7",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "pg": "^8.10.0",
    "puppeteer": "^23.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0"
  }
}
